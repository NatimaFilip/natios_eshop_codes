// Define SCSS variables for breakpoints
$small-mobile-max: 499px;
$large-mobile-min: 500px;
$mobile-max: 767px;
$tablet-min: 768px;
$small-tablet-max: 1023px;
$tablet-medium-min: 1024px;
$tablet-max: 1279px;
$desktop-min: 1280px;

$desktop-small-max: 1439px;
$desktop-medium-min: 1440px;
$desktop-medium-max: 1679px;
$desktop-large-min: 1680px;

@mixin primaryBackgroundGradient {
	--angle: 90deg;
	--start: 0%;
	--end: 100%;
	background: linear-gradient(var(--angle), var(--color-primary) var(--start), var(--color-primary-hover) var(--end));
}
@mixin tertiaryBackgroundGradient {
	--angle: 270deg;
	--start: 0%;
	--end: 100%;
	background: linear-gradient(var(--angle), var(--color-tertiary) var(--start), var(--color-tertiary-hover) var(--end));
}

@mixin secondaryBackgroundGradient {
	--angle: 90deg;
	--start: 0%;
	--end: 100%;
	background: linear-gradient(
		var(--angle),
		var(--color-secondary) var(--start),
		var(--color-secondary-hover) var(--end)
	);
}

@mixin list-reset {
	list-style: none;
	li {
		padding: 0.375em 0 0.375em 1.25em;
		position: relative;
		&:before {
			content: "";
			position: absolute;
			left: 0;
			top: 50%;
			transform: translateY(-50%);
			width: 0.375em;
			height: 0.375em;
			border-radius: 50%;
			background: var(--color-primary);
		}
	}
}

/*variables*/
